<footer class="menu">
  <div class="links">
    {{#each this.links as |link|}}
      {{#if link.route}}
        <LinkTo @route={{link.route}} class="menu-{{link.class}}" target={{link.target}}>
          <FaIcon @icon={{link.icon}} @prefix={{link.iconType}} />{{link.title}}
        </LinkTo>
      {{else}}
        <a href={{link.url}} class="menu-{{link.class}}" target={{link.target}}>
          <FaIcon @icon={{link.icon}} @prefix={{link.iconType}} />{{link.title}}
        </a>
      {{/if}}
    {{/each}}
  </div>
</footer>